vigation\/documentation\/MapboxNavigationCore\/CoreConfig","abstract":[{"text":"Mutable Core SDK configuration.","type":"text"}],"title":"CoreConfig","type":"topic","url":"\/documentation\/mapboxnavigationcore\/coreconfig","navigatorTitle":[{"kind":"identifier","text":"CoreConfig"}],"fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"CoreConfig","kind":"identifier"}],"kind":"symbol","role":"symbol"}}}